"    for i in range(n2):\n","        for j in range(n):\n","            Kmatrix[i, j] = k(X_hat[i], X[j])\n","\n","    return np.dot(Kmatrix, alpha)\n","\n","# Scaling input data and target values\n","scaler_X = StandardScaler()\n","X_scaled = scaler_X.fit_transform(X)\n","\n","scaler_y = StandardScaler()\n","y_scaled = scaler_y.fit_transform(y)\n","\n","# Regularization parameter lambda\n","lambda_reg = 1e-3\n","\n","# Compute Kernel Matrix with scaled data using the linear kernel\n","K = Km(X_scaled)\n","\n","# Solve for alpha using ridge regression\n","alpha = np.dot(np.linalg.inv(K + lambda_reg * np.eye(n)), y_scaled)\n","\n","# Make predictions on the first 20 points\n","y_hat_scaled = predictor(X_scaled[0:20, :], X_scaled, alpha)\n","\n","# Inverse transform predictions to original scale\n","y_hat = scaler_y.inverse_transform(y_hat_scaled)\n","\n","# Compare predicted vs actual values\n","comparison = np.hstack((y_hat, y[0:20, :]))\n","print(comparison)"],"metadata":{"id":"-eTnEPP8Ez7a","executionInfo":{"status":"aborted","timestamp":1743778266561,"user_tz":240,"elapsed":16620,"user":{"displayName":"Rishabh Saxena","userId":"01657484899872312362"}}},"execution_count":null,"outputs":[]}]}